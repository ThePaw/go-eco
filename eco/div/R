library(vegan)
x <- read.csv("data.csv", header = FALSE, sep = ",")
diversity(x, index = "shannon")
x <- read.csv("int.csv", header = FALSE, sep = ",")
diversity(x, index = "shannon")
diversity(x, index = "shannon", base = 2)
diversity(x, index = "shannon", base = 10)
diversity(x, index = "simpson")
diversity(x, index = "invsimpson")


