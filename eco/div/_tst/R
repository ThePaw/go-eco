y4 <- ineq(x4, parameter=1.0, type="entropy")
y5 <- ineq(x5, parameter=1.0, type="entropy")
y6 <- ineq(x6, parameter=1.0, type="entropy")

c(y1, y2, y3, y4, y5, y6)

