# Kolm 0.5
x1 <- c(4.637511, 5.795001, 5.700484, 6.524882, 6.170708, 6.690082, 6.156994, 6.921186, 4.740336, 5.400573)
x2 <- c(7.535951, 5.804745, 6.697524, 5.297671, 5.77213, 7.187614, 7.470511, 6.559553, 5.870524, 5.31025)
x3 <- c(4.428564, 5.698517, 4.882601, 6.541425, 4.910434, 7.311253, 4.562559, 6.858137, 7.105823, 5.963177)
x4 <- c(6.614128, 4.282033, 6.146613, 5.819527, 6.797518, 5.657332, 5.61468, 5.180996, 5.374655, 6.594351)
x5 <- c(6.79283, 6.371214, 5.990534, 6.518546, 6.283301, 6.841622, 5.978732, 5.278547, 7.825815, 6.36177)
x6 <- c(4.813066, 6.990308, 6.809527, 7.83582, 6.256215, 4.981545, 7.230944, 5.322504, 5.981109, 5.738691)

y1 <- ineq(x1, parameter=0.5, type="Kolm")
y2 <- ineq(x2, parameter=0.5, type="Kolm")
y3 <- ineq(x3, parameter=0.5, type="Kolm")
y4 <- ineq(x4, parameter=0.5, type="Kolm")
y5 <- ineq(x5, parameter=0.5, type="Kolm")
y6 <- ineq(x6, parameter=0.5, type="Kolm")

c(y1, y2, y3, y4, y5, y6)

y1 <- ineq(x1, parameter=1.0, type="Kolm")
y2 <- ineq(x2, parameter=1.0, type="Kolm")
y3 <- ineq(x3, parameter=1.0, type="Kolm")
y4 <- ineq(x4, parameter=1.0, type="Kolm")
y5 <- ineq(x5, parameter=1.0, type="Kolm")
y6 <- ineq(x6, parameter=1.0, type="Kolm")

c(y1, y2, y3, y4, y5, y6)

# Entropy 1.0
